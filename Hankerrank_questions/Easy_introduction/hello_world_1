#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
	
    char s[100];
    scanf("%[^\n]%*c", &s);
    printf("Hello, World!\n");
    printf("%s",s);
    
    return 0;
}

/* STD INPUT */
/* 
Welcome to C programming.  */

/* EXPECTED OUTPUT */
/* 
Hello, World!
Welcome to C programming.  */

/* MY OUTPUT */
/* 
Hello, World!
Welcome to C programming.  */
